{
  "name": "oaf",
  "version": "1.0.0",
  "description": "OAF, the discord bot for the Ascension Speedrun Society, reborn in typescript!",
  "repository": "https://github.com/Loathing-Associates-Scripting-Society/oaf-js.git",
  "main": "./dist/index.js",
  "type": "module",
  "scripts": {
    "start": "node --loader ts-node/esm/transpile-only ./src/index.ts",
    "format": "prettier --write src/**/*.ts",
    "prisma": "prisma",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "prepare": "husky install"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^4.13.0",
    "@types/node": "^18.16.3",
    "@xmldom/xmldom": "^0.8.7",
    "async-mutex": "^0.4.0",
    "axios": "^1.4.0",
    "date-fns": "^2.30.0",
    "discord.js": "^14.10.0",
    "dotenv": "^16.0.3",
    "html-entities": "^2.3.3",
    "otplib": "^12.0.1",
    "pretty-ms": "^8.0.0",
    "serialize-error": "^11.0.0",
    "sharp": "^0.32.1",
    "ts-dedent": "^2.2.0",
    "ts-node": "^10.9.1",
    "typed-emitter": "^2.1.0",
    "typescript": "^5.0.4",
    "typescript-memoize": "^1.1.1",
    "xpath": "^0.0.32"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^4.1.1",
    "@types/serialize-error": "^4.0.1",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.59.2",
    "@vitest/coverage-c8": "^0.32.2",
    "eslint": "^8.39.0",
    "husky": ">=6",
    "lint-staged": ">=10",
    "prettier": "^2.8.8",
    "prisma": "^4.13.0",
    "vitest": "^0.32.2"
  },
  "engines": {
    "node": "18.x"
  },
  "lint-staged": {
    "*.ts": "prettier --write"
  }
}
